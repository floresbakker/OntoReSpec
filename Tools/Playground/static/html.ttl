    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <hr> HTML element represents a thematic break between paragraph-level elements: for example, a change of scene in a story, or a shift of topic within a section."@en ;
    skos:prefLabel "the hr element"@en ;
    html:tag "hr" .

