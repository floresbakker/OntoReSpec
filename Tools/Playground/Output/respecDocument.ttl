    skos:prefLabel "the title element"@en ;
    html:tag "title" .

html:Embed a owl:Class ;
    dct:conformsTo section1:4.8.6 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <embed> HTML element embeds external content at the specified point in the document. This content is provided by an external application or other source of interactive content such as a browser plug-in."@en ;
