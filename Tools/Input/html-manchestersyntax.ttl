    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path dom:documentType .

shp:Document_root a sh:PropertyShape ;
    rdfs:label "has root element html"@en ;
    rdfs:comment "The <html> tag is the root of the HTML document and a container for all other HTML elements (except for the <!DOCTYPE> tag). A HTML document contains no less and no more than one html tag."@en ;
    sh:class html:Html ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path rdfs:member .

shp:Element_attribute a sh:PropertyShape ;
    rdfs:comment "Every element can have one or more attributes."@en ;
    skos:prefLabel "has attribute shape"@en ;
    sh:nodeKind sh:Literal ;
    sh:path html:attribute .

shp:Element_fragment a sh:PropertyShape ;
    rdfs:comment "Every element can have one and only one html fragment."@en ;
    skos:prefLabel "has fragment shape"@en ;
    sh:datatype rdf:HTML ;
    sh:path html:fragment .

shp:Element_style a sh:PropertyShape ;
    rdfs:label "has style attribute"@en ;
    rdfs:comment "Every element can have at most one style attribute."@en ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:path html:style .

shp:Head_styleSheet a sh:PropertyShape ;
    rdfs:label "has style sheet"@en ;
    rdfs:comment "A head of an html document, may have one embedded style sheet, which is comprised of presentational and formatting instructions for the rest of the document, and the expected input is CSS declarations."@en ;
    sh:class html:StyleSheet ;
    sh:maxCount 1 ;
    sh:path rdfs:member .

shp:Head_title a sh:PropertyShape ;
    rdfs:label "has title"@en ;
    rdfs:comment "A head of an html document, may have one title element."@en ;
    sh:class html:Title ;
    sh:maxCount 1 ;
    sh:path rdfs:member .

shp:HeaderRow_member a sh:PropertyShape ;
    skos:prefLabel "has header cell"@en ;
    sh:class html:HeaderCell ;
    sh:path rdfs:member .

shp:Html_body a sh:PropertyShape ;
    rdfs:label "has body"@en ;
    rdfs:comment "A html root element may have one and only one body element."@en ;
    sh:class html:Body ;
    sh:maxCount 1 ;
    sh:path rdfs:member .

shp:Html_head a sh:PropertyShape ;
    rdfs:label "has head"@en ;
    rdfs:comment "An html document, may have one and only one head element, which is the first element of the document."@en ;
    sh:class html:Head ;
    sh:maxCount 1 ;
    sh:path rdfs:member .

shp:TableBody_member a sh:PropertyShape ;
    skos:prefLabel "has data row"@en ;
    sh:class html:DataRow ;
    sh:path rdfs:member .

shp:TableHeader_member a sh:PropertyShape ;
    skos:prefLabel "has header row"@en ;
    sh:class html:HeaderRow ;
    sh:path rdfs:member .

shp:Table_caption a sh:PropertyShape ;
    rdfs:comment "Tables may have a caption."@en ;
    skos:prefLabel "has caption shape"@en ;
    sh:class html:Caption ;
    sh:maxCount 1 ;
    sh:path rdfs:member .

shp:Table_tbody a sh:PropertyShape ;
    rdfs:comment "Tables may have zero or more table bodies."@en ;
    skos:prefLabel "has table body"@en ;
    sh:class html:TableBody ;
    sh:path rdfs:member .

shp:Table_tfoot a sh:PropertyShape ;
    rdfs:comment "Tables may have a footer."@en ;
    skos:prefLabel "has table footer shape"@en ;
    sh:class html:TFoot ;
    sh:maxCount 1 ;
    sh:path rdfs:member .

shp:Table_thead a sh:PropertyShape ;
    rdfs:comment "Tables may have a header."@en ;
    skos:prefLabel "has table header shape"@en ;
    sh:class html:TableHeader ;
    sh:maxCount 1 ;
    sh:path html:thead .

target:Document a sh:SPARQLTarget ;
    sh:select """prefix html: <https://data.rijksfinancien.nl/html/model/def/>
select $this {
  $this a html:Document.
  filter not exists {
    $this html:fragment [].
  }
}""" .

target:SerializableElement a sh:SPARQLTarget ;
    sh:select """prefix dom: <https://data.rijksfinancien.nl/dom/model/def/>
prefix html: <https://data.rijksfinancien.nl/html/model/def/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

# Select the HTML element for which there does not yet exist an HTML
# fragment, with the additional requirement that each and any
# underlying child element contains an HTML fragment.
select $this {
  # Any DOM element (`dom:Element`) that is not known to be a void element
  # (html:VoidElement).
  $this a/rdfs:subClassOf* dom:Element.
  filter not exists {
    $this a/rdfs:subClassOf html:VoidElement.
  }
  # The HTML element must not have an HTML fragment yet.
  filter not exists {
    $this html:fragment [].
  }
  # The HTML element may not contain one or more child elements that lack an
  # HTML fragment.
  minus {
    $this ?member ?childElement.
    ?childElement a/rdfs:subClassOf* dom:Element.
    filter not exists {
      ?childElement html:fragment [].
    }
  }
}""" .

target:VoidElement a sh:SPARQLTarget ;
    sh:select """prefix html: <https://data.rijksfinancien.nl/html/model/def/>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
select $this {
  $this a/rdfs:subClassOf* html:VoidElement.
  filter not exists {
    $this html:fragment [].
  }
}""" .

section:13.1.2.3 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#attributes-2"^^xsd:anyURI ;
    skos:prefLabel "§ 13.1.2.3 Attributes"@en .

section:4.12.3 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#the-template-element"^^xsd:anyURI ;
    skos:prefLabel "§ 4.12.3 The template element"@en .

section:4.5.19 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#the-sub-and-sup-elements"^^xsd:anyURI ;
    skos:prefLabel "§ 4.5.19 The sub and sup elements"@en .

html:Area a owl:Class ;
    dct:conformsTo section:4.8.13 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <area> HTML element defines an area inside an image map that has predefined clickable areas. An image map allows geometric areas on an image to be associated with hypertext link."@en ;
    skos:prefLabel "area element"@en ;
    html:tag "area" ;
    ns1:definition "SUBCLASSOF Void element" ;
    ns1:label "area element" .

html:Body a owl:Class ;
    dct:conformsTo section:4.3.1 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <body> HTML element is the second element in a root html element. It contains the contents of the document."@en ;
    skos:prefLabel "body element"@en ;
    html:tag "body" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "body element" .

html:Caption a owl:Class ;
    dct:conformsTo section:4.9.2 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The caption element represents the title of the table that is its parent, if it has a parent and that is a table element."@en ;
    skos:prefLabel "caption"@en ;
    html:tag "caption" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "caption" .

html:Embed a owl:Class ;
    dct:conformsTo section:4.8.6 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <embed> HTML element embeds external content at the specified point in the document. This content is provided by an external application or other source of interactive content such as a browser plug-in."@en ;
    skos:prefLabel "embed element"@en ;
    html:tag "embed" ;
    ns1:definition "SUBCLASSOF Void element" ;
    ns1:label "embed element" .

html:Head a owl:Class ;
    dct:conformsTo section:4.2.1 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <head> HTML element contains machine-readable information (metadata) about the document, like its title, scripts, and style sheets."@en ;
    skos:prefLabel "head element"@en ;
    html:tag "head" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "head element" .

html:Html a owl:Class ;
    dct:conformsTo section:4.1.1 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <html> HTML element represents the root (top-level element) of an HTML document, so it is also referred to as the root element. All other elements must be descendants of this element."@en ;
    skos:prefLabel "html element"@en ;
    html:tag "html" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "html element" .

html:Iframe a owl:Class ;
    dct:conformsTo section:4.8.5 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <iframe> HTML element represents a nested browsing context, embedding another HTML page into the current one."@en ;
    skos:prefLabel "iframe element"@en ;
    html:tag "iframe" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "iframe element" .

html:Source a owl:Class ;
    dct:conformsTo section:4.8.2 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <source> HTML element specifies multiple media resources for the <picture>, the <audio> element, or the <video> element. It is an empty element, meaning that it has no content and does not have a closing tag. It is commonly used to offer the same media content in multiple file formats in order to provide compatibility with a broad range of browsers given their differing support for image file formats and media file formats."@en ;
    skos:prefLabel "source element"@en ;
    html:tag "source" ;
    ns1:definition "SUBCLASSOF Void element" ;
    ns1:label "source element" .

html:TableBody a owl:Class ;
    dct:conformsTo section:4.9.5 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The table body (<tbody> element) represents a block of rows that consist of a body of data for the parent table."@en ;
    skos:prefLabel "table body"@en ;
    html:tag "tbody" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "table body" .

html:TableHeader a owl:Class ;
    dct:conformsTo section:4.9.6 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The table header (<thead> element) represents the block of rows that consist of the column labels (headers) for the parent table."@en ;
    skos:prefLabel "table header"@en ;
    html:tag "thead" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "table header" .

html:Title a owl:Class ;
    dct:conformsTo section:4.2.2 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The Title (<title> element) defines a document its title that is shown in a browser title bar or a page tab. It only contains text; tags within the element are ignored."@en ;
    skos:prefLabel "title"@en ;
    html:tag "title" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "title" .

html:Video a owl:Class ;
    dct:conformsTo section:4.8.8 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <video> HTML element embeds a media player which supports video playback into the document. You can use <video> for audio content as well, but the <audio> element may provide a more appropriate user experience."@en ;
    skos:prefLabel "video element"@en ;
    html:tag "video" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "video element" .

section:4.9.8 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#the-tr-element"^^xsd:anyURI ;
    skos:prefLabel "§ 4.9.8 The tr element"@en .

html:A a owl:Class ;
    dct:conformsTo section:4.5.1 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <a> HTML element (or anchor element), with its href attribute, creates a hyperlink to web pages, files, email addresses, locations in the same page, or anything else a URL can address."@en ;
    skos:prefLabel "a element"@en ;
    html:tag "a" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "a element" .

html:Cell a owl:Class ;
    rdfs:subClassOf html:Element ;
    owl:unionOf ( html:DataCell html:HeaderCell ) ;
    skos:prefLabel "cell"@en ;
    ns1:definition "SUBCLASSOF element",
        "data cell OR header cell" ;
    ns1:label "cell" .

html:DataCell a owl:Class ;
    dct:conformsTo section:4.9.9 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The data cell (<td> element) represents a cell in a table that contains some unspecified form of data."@en ;
    skos:prefLabel "data cell"@en ;
    html:tag "td" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "data cell" .

html:DataRow a owl:Class ;
    dct:conformsTo section:4.9.8 ;
    rdfs:subClassOf html:Element ;
    skos:prefLabel "data row"@en ;
    html:tag "tr" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "data row" .

html:HeaderCell a owl:Class ;
    dct:conformsTo section:4.9.10 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The ‘th’ element represents a header cell in a table."@en ;
    skos:prefLabel "header cell"@en ;
    html:tag "th" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "header cell" .

html:HeaderRow a owl:Class ;
    dct:conformsTo section:4.9.8 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The table header row (<tr> element) represents a row of cells in a table header."@en ;
    skos:prefLabel "header row"@en ;
    html:tag "tr" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "header row" .

html:Img a owl:Class ;
    dct:conformsTo section:4.8.3 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <img> HTML element embeds an image into the document."@en ;
    skos:prefLabel "img element"@en ;
    html:tag "img" ;
    ns1:definition "SUBCLASSOF Void element" ;
    ns1:label "img element" .

html:Link a owl:Class ;
    dct:conformsTo section:4.2.4 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The link element allows authors to link their document to other resources."@en ;
    skos:prefLabel "link element"@en ;
    html:tag "link" ;
    ns1:definition "SUBCLASSOF Void element" ;
    ns1:label "link element" .

html:StyleSheet a owl:Class ;
    dct:conformsTo section:4.2.6 ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <style> HTML element contains style information for a document, or part of a document. It embeds a CSS style sheet, which is applied to the contents of the document containing the <style> element."@en ;
    skos:prefLabel "style sheet"@en ;
    html:tag "style" ;
    ns1:definition "SUBCLASSOF element" ;
    ns1:label "style sheet" .

html:Meta a owl:Class ;
    dct:conformsTo section:4.2.5 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <meta> HTML element can represent document-level metadata with the name attribute, pragma directives with the http-equiv attribute, and the file's character encoding declaration when an HTML document is serialized to string form (e.g. for transmission over the network or for disk storage) with the charset attribute."@en ;
    skos:prefLabel "meta element"@en ;
    html:tag "meta" ;
    ns1:definition "SUBCLASSOF Void element" ;
    ns1:label "meta element" .

section:4.3.6 a dcm:Text ;
    dct:isPartOf standard:html ;
    dct:source "https://html.spec.whatwg.org/#the-h1,-h2,-h3,-h4,-h5,-and-h6-elements"^^xsd:anyURI ;
    skos:prefLabel "§ 4.3.6 The h1, h2, h3, h4 and h6 elements"@en .

html:Input a owl:Class ;
    dct:conformsTo section:4.10.5 ;
    rdfs:subClassOf html:VoidElement ;
    skos:definition "The <input> HTML element is used to create interactive controls for web-based forms in order to accept data from the user; a wide variety of types of input data and control widgets are available, depending on the device and user agent. The <input> element is one of the most powerful and complex in all of HTML due to the sheer number of combinations of input types and attributes."@en ;
    skos:prefLabel "input element"@en ;
    html:tag "input" ;
    ns1:definition "SUBCLASSOF Void element" ;
    ns1:label "input element" .

html:VoidElement a owl:Class ;
    rdfs:subClassOf dom:Element ;
    skos:definition "A DOM element whose content model never allows it to have contents under any circumstances. Void elements can have attributes."@en ;
    skos:example "<br>"@en,
        "<hr>"@en ;
    skos:prefLabel "Void element"@en ;
    ns1:definition "SUBCLASSOF Element" ;
    ns1:label "Void element" .

html:attribute a owl:DatatypeProperty ;
    dct:conformsTo section:13.1.2.3 ;
    rdfs:domain html:Element ;
    rdfs:range rdfs:Literal ;
    skos:definition "An attribute is a name-value pair that is associated with an HTML element. Attributes provide additional information about an element and are specified within the start tag of an element. Attributes can modify the behavior or appearance of an element, define relationships between elements, or provide other metadata. The name of the attribute is followed by an equal sign (=) and the attribute's value, which is enclosed in double or single quotes. Some attributes affect the element simply by their presence in the start tag of the element, with the value implicitly being an empty string. Html:atribute is the parent property of all existing HTML attribute properties."@en ;
    skos:prefLabel "has attribute"@en ;
    ns1:definition "Domain:  element" ;
    ns1:label "Property name:  has attribute" .

standard:html a dct:Standard ;
    dct:created "2021-11-09"^^xsd:date ;
    dct:source "https://html.spec.whatwg.org"^^xsd:anyURI ;
    skos:prefLabel "HTML Living Standard"@en .

html:Element a owl:Class ;
    dct:conformsTo section:3.2 ;
    rdfs:subClassOf prov:Entity,
        dom:Element ;
    skos:definition "An element in the DOM represents a thing; that is, it has intrinsic meaning, also known as semantics. An element consists of an HTML start tag and an HTML end tag and has value content. An HTML start tag consists of a ‘smaller than’ character (‘<’) and a tag name, followed by a ‘greater than’ character (‘>’). An HTML end tag consists of a ‘smaller than’ character (‘<’), a slash (‘/’) and a tag name, followed by a ‘greater than’ character (‘>’).  The value content of an element can be arbitrarily complex."@en ;
    skos:prefLabel "element"@en ;
    ns1:definition "SUBCLASSOF Element",
        "SUBCLASSOF Entity" ;
    ns1:label "element" .

