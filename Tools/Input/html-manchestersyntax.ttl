    skos:prefLabel "the output element"@en ;
    html:tag "output" ;
    manchester:label "[=the output element=]" ;
    manchester:subClassOf "[=HTML DOM element=]" ;
    manchester:syntax "SUBCLASSOF" ;
    manchester:type "CLASS" .

html:Row a owl:Class ;
