html:Dfn a owl:Class ;
    dct:conformsTo section:4.5.8 ;
    rdfs:isDefinedBy html: ;
    rdfs:subClassOf html:Element ;
    skos:definition "The <dfn> HTML element is used to indicate the term being defined within the context of a definition phrase or sentence. The <p> element, the <dt>/<dd> pairing, or the <section> element which is the nearest ancestor of the <dfn> is considered to be the definition of the term."@en ;
    skos:prefLabel "the dfn element"@en ;
    html:tag "dfn" ;
    manchester:label "[=the dfn element=]" ;
